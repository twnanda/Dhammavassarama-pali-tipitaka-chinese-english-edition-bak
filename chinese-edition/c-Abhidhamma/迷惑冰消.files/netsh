<!--
	function tophotPlayAdSub()	//tophot里面的轮播广告
	{
		if(document.getElementById("tophotOrderAd") != null && tophotAdInfo != undefined)
		{
			document.getElementById("tophotOrderAd").innerHTML = tophotAdInfo[tophotAdSeq];
			tophotAdSeq++;
			if(tophotAdSeq == tophotAdInfo.length)
			{
				tophotAdSeq = 0;
			}
		}
		setTimeout("tophotPlayAdSub();",5000);
	}
	document.write(ad_p4_sp0_n30);
	//henry 2007-1-5 暂时拿掉
	//tophotPlayAdSub();
