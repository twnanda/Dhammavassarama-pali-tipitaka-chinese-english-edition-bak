	function subSearch(form)
	{
		switch(form.sel_sSearchType.value)
		{
			case "note":
				form.action = "/eden/forum/searchforumnote/index.php";
				break;
			default:
				form.action = "/eden/combo/ctl_eden_search.php?ctlAct="+ form.sel_sSearchType.value;
				break;
		}
		return true;
	}
	//2007-3-21 henry add for video program
	function showVideoNew(p_iVideoNum)
	{
		//alert(p_iVideoNum);
		for(i=0; i<5; i++)
		{
			document.getElementById("divVideoNewPic"+i).style.display = "none";
			document.getElementById("divVideoNewControl"+i).style.display = "none";
		}
		document.getElementById("divVideoNewPic"+p_iVideoNum).style.display = "block";
		document.getElementById("divVideoNewControl"+p_iVideoNum).style.display = "block";
	}

	function showVideoBrowse(p_iVideoNum)
	{
		//alert(p_iVideoNum);
		for(i=0; i<5; i++)
